{% extends 'admin/base.html.twig' %}
{% block title %}Ajout d'une province{% endblock %}

{% block body %}
    {% block content %}
        <div class="container">
            <h1 class="my-5">Ajout d'une province</h1>

            {{ form_start(form) }}
            {{ form_widget(form) }}
            <button type="submit" class="btn btn-primary">
                <i class="icon-ok">Ajouter la province ! </i>

            </button>
            {{ form_end(form) }}
        </div>
    {% endblock %}
{% endblock %}